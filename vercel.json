{
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "installCommand": "npm install",
    "buildCommand": "echo 'No build step required for this Node.js server'",
    "outputDirectory": "public"
}